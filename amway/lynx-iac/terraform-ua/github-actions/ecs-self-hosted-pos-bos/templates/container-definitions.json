[
  {
    "essential": false,
    "image": "${init_image}",
    "name": "init-actions",
    "cpu": 0,
    "memoryReservation": 64,
    "environment": [
      {
        "name": "GIT_ORG",
        "value":"${git_org}"
      },
      {
        "name": "GIT_REPO",
        "value":"${git_repo}"
      },
      {
        "name": "GIT_TOKEN",
        "value": "${git_token}"
      },
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "eu-central-1"
      }
    ],
    "mountPoints": [],
    "volumesFrom": [],
    "privileged": null
  },
  {
    "essential": true,
    "dependsOn": [
      {
        "containerName": "init-actions",
        "condition": "SUCCESS"
      }
    ],
    "image": "${runner_image}",
    "name": "actions-runner",
    "cpu": 0,
    "memoryReservation": ${runner_memory_soft},
    "environment": [
      {
        "name": "GIT_ORG",
        "value": "${git_org}"
      },
      {
        "name": "GIT_REPO",
        "value": "${git_repo}"
      },
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "eu-central-1"
      },
      {
        "name": "LABELS",
        "value": "${labels}"
      }
    ]
  }
]
