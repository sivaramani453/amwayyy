{
    "Version": "2012-10-17",
    "Id": "AllowGetObjFromHybrisVPC",
    "Statement": [
        {
            "Sid": "AllowS3GetObject",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "${resource_arn}",
            "Condition": {
                "StringLike": {
                    "aws:UserAgent": "${user_agent}"
                },
                "IpAddress": {
                    "aws:SourceIp": "${ip_address}"
                }
            }
        }
    ]
}
